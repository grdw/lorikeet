---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Lorikeet</title>
 <link href="http://lorikeet.nl/atom.xml" rel="self"/>
 <link href="http://lorikeet.nl/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://lorikeet.nl/</id>
 <author>
   <name>Gerard Westerhof</name>
   <email>bird@lorikeet.nl</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://lorikeet.nl{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://lorikeet.nl{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
